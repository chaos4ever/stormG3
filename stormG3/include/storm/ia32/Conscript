Import qw(cons header_directory);

$headers = "memory.h port.h";

Command $cons 'install', '', "mkdir -p $header_directory/storm/ia32 && for e in $headers ; do install include/storm/ia32/\$e $header_directory/storm/ia32 || exit ; done"
